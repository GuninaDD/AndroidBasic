<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    android:orientation="vertical"
    tools:context=".TownChangeFragment">

    <EditText
        android:id="@+id/et_town"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_search"
        android:layout_marginStart="@dimen/spacing_xxsmall"
        android:layout_marginTop="@dimen/spacing_xxsmall"
        android:layout_marginEnd="@dimen/spacing_xxsmall"
        android:autofillHints=""
        android:inputType="none|text"
        android:labelFor="@id/et_town">

    </EditText>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_xxsmall"
        android:layout_marginTop="@dimen/spacing_xsmall"
        android:layout_marginEnd="@dimen/spacing_xxsmall"
        android:orientation="horizontal">

        <CheckBox
            android:id="@+id/cb_wing"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="@string/cb_wing" />

        <CheckBox
            android:id="@+id/cb_pressure"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/cb_pressure" />
    </LinearLayout>

</LinearLayout>